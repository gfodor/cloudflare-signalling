{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "moduleDetection": "force",
    "lib": ["ES2022"],
    "strict": true,
    "types": ["@cloudflare/workers-types"],
    "allowJs": false,
    "checkJs": false,
    "noEmit": true
  },
  "include": ["src/**/*", "test/**/*"]
}
